public class Main {

    public static void main(String[] args) {
    Scanner scn = new Scanner(System.in);
    int a[]=new int[4];
    for(int i=0;i<a.length;i++){
         a[i]= scn.nextInt();
    }
    D(a);
    }
    public static void D(int arr[]){
        for (int i=0;i<arr.length;i++){
            System.out.print(arr[i]*arr[i]+"\t");
        }
        System.out.println();
    }
}

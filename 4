import java.util.Scanner;

public class Main {

    public static void main(String[] args) {
        Scanner scn = new Scanner(System.in);
        int a =scn.nextInt();
        int b =scn.nextInt();
        F(a,b);
    }
    public static void F(int w,int h){
        for(int i=1;i<=h;i++){
            for(int j=1;j<=w;j++){
                System.out.print(i*j+"\t");
            }
            System.out.println();
        }
    }
}
